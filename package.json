{
  "private": true,
  "name": "reactrouter-dot-com",
  "description": "",
  "license": "",
  "prisma": {
    "seed": "ts-node app/seed.ts"
  },
  "scripts": {
    "build:css": "postcss styles --base styles --dir app/styles --env production",
    "dev:css": "postcss styles --base styles --dir app/styles -w",
    "build:remix": "NODE_ENV=production remix build",
    "build": "run-s build:*",
    "dev": "pm2-dev pm2.config.js",
    "deploy": "flyctl deploy --config fly.staging.toml --build-arg REMIX_TOKEN=${REMIX_TOKEN} --build-arg DATABASE_URL=file:./dev.db?connection_limit=1",
    "deploy:localdb": "flyctl deploy --config fly.staging.toml --build-arg REMIX_TOKEN=${REMIX_TOKEN} --build-arg DATABASE_URL=file:./dev.db?connection_limit=1 --build-arg SKIP_RESET=1",
    "deploy:production:localdb": "flyctl deploy --config fly.production.toml --build-arg REMIX_TOKEN=${REMIX_TOKEN} --build-arg DATABASE_URL=file:./dev.db?connection_limit=1 --build-arg SKIP_RESET=1",
    "start": "NODE_ENV=production node --max-old-space-size=900 server/index.js",
    "postinstall": "remix setup && patch-package",
    "clean": "del app/styles .cache public/build server/build",
    "db:reset": "prisma migrate reset --force && ts-node app/verify-seed"
  },
  "dependencies": {
    "@prisma/client": "3.1.1",
    "@reach/rect": "^0.16.0",
    "@reach/skip-nav": "0.16.0",
    "@remix-run/node": "0.19.3",
    "@remix-run/react": "0.19.3",
    "@remix-run/serve": "0.19.3",
    "@ryanflorence/md": "^6.1.2",
    "@ryanflorence/mdtut": "^1.2.1",
    "accept-language": "3.0.18",
    "clsx": "1.1.1",
    "follow-redirects": "1.14.4",
    "gray-matter": "4.0.3",
    "gunzip-maybe": "1.4.2",
    "lodash.merge": "^4.6.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router-dom": "6.0.0-beta.6",
    "remark": "^13.0.0",
    "remark-html": "^13.0.1",
    "remix": "0.19.3",
    "semver": "7.3.5",
    "tar-stream": "2.2.0",
    "tiny-invariant": "1.1.0",
    "ts-is-present": "1.2.2",
    "use-throttle": "^0.0.3"
  },
  "devDependencies": {
    "@remix-run/dev": "0.19.3",
    "@tailwindcss/aspect-ratio": "0.2.1",
    "@tailwindcss/typography": "0.4.1",
    "@types/follow-redirects": "1.13.1",
    "@types/gunzip-maybe": "1.4.0",
    "@types/lodash.merge": "^4.6.6",
    "@types/react": "17.0.24",
    "@types/react-dom": "17.0.9",
    "@types/semver": "7.3.8",
    "@types/tailwindcss": "^2.2.1",
    "@types/tar-stream": "2.2.1",
    "chokidar": "3.5.2",
    "concurrently": "^6.2.1",
    "del-cli": "4.0.1",
    "dotenv": "10.0.0",
    "npm-run-all": "4.1.5",
    "patch-package": "6.4.7",
    "pm2": "5.1.2",
    "postcss": "^8.3.9",
    "postcss-100vh-fix": "1.0.2",
    "postcss-cli": "^9.0.1",
    "postcss-focus-visible": "^6.0.1",
    "postcss-import": "^14.0.2",
    "postcss-nesting": "^8.0.1",
    "prisma": "3.1.1",
    "tailwindcss": "2.2.16",
    "ts-node": "^10.3.0",
    "typescript": "4.4.3"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
