{
  "private": true,
  "name": "reactrouter-dot-com",
  "description": "",
  "license": "",
  "scripts": {
    "build:css": "concurrently \"tailwindcss --input ./styles/tailwind.css --output ./app/styles/tailwind.css --minify --postcss\" \"tailwindcss --input ./styles/docs.css --output ./app/styles/docs.css --minify --postcss\"",
    "dev:css": "concurrently \"tailwindcss --input ./styles/tailwind.css --output ./app/styles/tailwind.css --watch --postcss\" \"tailwindcss --input ./styles/docs.css --output ./app/styles/docs.css --watch --postcss\"",
    "build:remix": "NODE_ENV=production remix build",
    "build": "run-s build:*",
    "dev": "pm2-dev pm2.config.js",
    "deploy": "flyctl deploy --build-arg REMIX_TOKEN=${REMIX_TOKEN}",
    "start": "NODE_ENV=production node server",
    "postinstall": "remix setup && patch-package",
    "clean": "del app/styles .cache public/build server/build"
  },
  "dependencies": {
    "@prisma/client": "3.1.1",
    "@reach/rect": "^0.16.0",
    "@reach/skip-nav": "0.16.0",
    "@remix-run/node": "0.0.0-experimental-2e859e2",
    "@remix-run/react": "0.0.0-experimental-2e859e2",
    "@remix-run/serve": "0.0.0-experimental-2e859e2",
    "@ryanflorence/md": "^5.2.0",
    "accept-language": "3.0.18",
    "clsx": "1.1.1",
    "follow-redirects": "1.14.4",
    "gray-matter": "4.0.3",
    "gunzip-maybe": "1.4.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router-dom": "6.0.0-beta.3",
    "remix": "0.0.0-experimental-2e859e2",
    "semver": "7.3.5",
    "tailwindcss": "2.2.16",
    "tar-stream": "2.2.0",
    "tiny-invariant": "1.1.0"
  },
  "devDependencies": {
    "@remix-run/dev": "0.0.0-experimental-2e859e2",
    "@tailwindcss/aspect-ratio": "0.2.1",
    "@tailwindcss/typography": "0.4.1",
    "@types/follow-redirects": "1.13.1",
    "@types/gunzip-maybe": "1.4.0",
    "@types/react": "17.0.24",
    "@types/react-dom": "17.0.9",
    "@types/semver": "7.3.8",
    "@types/tailwindcss": "^2.2.1",
    "@types/tar-stream": "2.2.1",
    "concurrently": "^6.2.1",
    "del-cli": "4.0.1",
    "dotenv": "10.0.0",
    "npm-run-all": "4.1.5",
    "patch-package": "6.4.7",
    "pm2": "5.1.2",
    "postcss-100vh-fix": "1.0.2",
    "postcss-focus-visible": "^6.0.1",
    "postcss-import": "^14.0.2",
    "postcss-nesting": "^8.0.1",
    "prisma": "3.1.1",
    "typescript": "4.4.3"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
