    "@remix-run/serve": "2.8.1",
    "bcryptjs": "^2.4.3",
    "cheerio": "^1.0.0-rc.12",
    "classnames": "^2.3.2",
    "escape-goat": "^4.0.0",
    "follow-redirects": "^1.15.2",
    "front-matter": "^4.0.2",
    "gray-matter": "^4.0.3",
    "gunzip-maybe": "^1.4.2",
    "isbot": "^3.6.10",
    "iso-639-1": "^2.1.15",
    "lodash.merge": "^4.6.2",
    "lru-cache": "^7.18.3",
    "octokit": "^2.0.14",
    "parse-link-header": "^2.0.0",
    "parse-numeric-range": "^1.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.14.2",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-slug": "^6.0.0",
    "rehype-stringify": "^10.0.0",
    "remark-gfm": "^4.0.0",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.0",
    "semver": "^7.5.0",
    "shiki": "^0.14.7",
    "sort-by": "^1.2.0",
    "tar": "^6.1.14",
    "tar-stream": "^3.0.0",
    "tiny-invariant": "^1.3.1",
    "unified": "^11.0.4",
    "unist-util-visit": "^5.0.0"
